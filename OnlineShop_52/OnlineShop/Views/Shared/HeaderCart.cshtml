@model List<OnlineShop.Models.CartItem>
<div class="header-right2">
    <div class="cart box_1">
        <a href="checkout.html">
            <h3>
                <div class="total">
                    <span class="simpleCart_total"></span> (<span id="simpleCart_quantity" class="simpleCart_quantity">@Model.Count</span> sản phẩm)
                </div>
                <img src="~/assets/clientNew/images/bag.png" alt="" />
            </h3>
        </a>
        <p><a href="javascript:;" class="simpleCart_empty">Giỏ hàng trống</a></p>
        <div class="clearfix"> </div>
    </div>
</div>


